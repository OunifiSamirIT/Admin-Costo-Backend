

  {
    "version": 2,
    "builds": [
      {
        "src": "nest/package.json",
        "use": "@vercel/node",
        "config": {
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/nest/$1" }
    ],
    "env": {
      "NODE_ENV": "production",
      "PORT": "5000",

      "DATABASE_URL": "mysql://root:@localhost:3306/CostoEcommerce"
    }
  }
  