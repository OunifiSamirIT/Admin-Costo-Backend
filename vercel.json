

  {
    "version": 2,
    "builds": [
      {
        "src": "nest/package.json",
        "use": "@vercel/node",
        "config": {
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/dist/$1" }
    ],
    "envDevelopment": {
      "NODE_ENV": "development",
      "DATABASE_URL": "mysql://root:@localhost:3306/CostoEcommerce"
    },
    "build": {
      "env": {
        "NODE_ENV": "production"
      },
      "commands": {
        "build": "npm run prisma:generate && npm run build"
      }
    }
  }
  